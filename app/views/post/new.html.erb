<div class="post_form">
  <%= render 'shared/errors', object: @post %>

  <%= form_for @post do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
    <%= f.label :annonce %><br>
    <%= f.text_area :annonce %><br>
    <%= f.label :text %><br>
    <%= f.text_area :text %><br>
    <%= f.label :active %><br>
    <%= f.check_box :active %><br>
    <%= f.submit %>
  <%end%>
</div>
